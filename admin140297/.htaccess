RewriteEngine on


#------------------------------------------------------------------------------------
# Règles de réécriture d'URL :
#------------------------------------------------------------------------------------

#--------- pages de la maquete ---------------
RewriteRule ^modifier,(.*)$ modifier.php?reference=$1 [L]
