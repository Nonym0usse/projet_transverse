# Activation du module de r��criture d'URL :
RewriteEngine on


#------------------------------------------------------------------------------------
# Règles de réécriture d'URL :
#------------------------------------------------------------------------------------

#--------- pages de la maquete ---------------
RewriteRule ^article$ index.php?op=list_articles [L]
